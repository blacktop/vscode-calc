{
	"name": "vscode-calc",
	"displayName": "Programmer Calculator",
	"description": "Programmer Calculator extension for Visual Studio Code",
	"publisher": "blacktop",
	"author": {
		"name": "Blacktop"
	},
	"license": "Apache-2.0",
	"version": "0.0.1",
	"engines": {
		"vscode": "^1.61.0"
	},
	"categories": [
		"Debuggers",
		"Other"
	],
	"galleryBanner": {
		"color": "#fd8d0e",
		"theme": "dark"
	},
	"private": true,
	"repository": {
		"type": "git",
		"url": "https://github.com/blacktop/vscode-calc"
	},
	"bugs": {
		"url": "https://github.com/blacktop/vscode-calc/issues"
	},
	"keywords": [
		"pcalc calc calulator programmer"
	],
	"activationEvents": [
		"onCommand:vscode-calc.helloWorld"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "vscode-calc.helloWorld",
				"title": "Hello World"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile && npm run lint",
		"lint": "eslint src --ext ts",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/vscode": "^1.61.0",
		"@types/glob": "^7.1.4",
		"@types/mocha": "^9.0.0",
		"@types/node": "14.x",
		"@typescript-eslint/eslint-plugin": "^4.31.1",
		"@typescript-eslint/parser": "^4.31.1",
		"eslint": "^7.32.0",
		"glob": "^7.1.7",
		"mocha": "^9.1.1",
		"typescript": "^4.4.3",
		"@vscode/test-electron": "^1.6.2"
	}
}